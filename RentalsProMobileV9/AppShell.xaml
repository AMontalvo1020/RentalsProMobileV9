<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RentalsProMobileV9.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RentalsProMobileV9"
    xmlns:views="clr-namespace:RentalsProMobileV9"
    Shell.FlyoutBehavior="Flyout"
    Title="RentalsProMobileV9">

    <FlyoutItem>
        <ShellContent ContentTemplate="{DataTemplate views:LoginPage}" Route="Login" />
    </FlyoutItem>

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
